func nb-paranja(nodeset options) {
    default = {
        'theme': 'dark'
    }

    nb-block('paranja', nb-extend(default, options))
}

match .paranja nb {
    <div>
        _size-theme-type-class({
           'name': 'paranja'
           'theme': .theme
        })

        apply . nb-main-attrs
        
        if .content {
            html( .content )
        }
    </div>
}
